<h1>New song</h1>

<%= form_for @song do |f| %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :song_title %>
  <%= f.text_field :song_title %>

  <%= f.label :artist_id %>
  <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>

  <%= f.submit "Create" %>
<% end %>
